from company import Company
from staff import Staff

def main():
    print("=" * 60)
    print("ç¤¾å“¡ç®¡ç†ã‚·ã‚¹ãƒ†ãƒ ")
    print("=" * 60)
    
    company = Company()
    
    # ç¤¾å“¡è¿½åŠ 
    staff1 = Staff()
    staff1.set_name("ä½è—¤å¤ªéƒ")
    staff1.set_salary(200000)
    staff1.set_division("å–¶æ¥­éƒ¨")
    company.staffs.append(staff1)
    
    # æƒ…å ±è¡¨ç¤º
    for staff in company.staffs:
        print(f"åå‰ï¼š{staff.get_name()}ã€"
              f"çµ¦æ–™ï¼š{staff.get_salary()}å††ã€"
              f"æ‰€å±ï¼š{staff.get_division()}")

